                    space8()
                elif ob==[1,5,6,7,8] or ob==[1,6,5,7,8]:
                    space2()
                elif ob==[1,5,8,7,4] or ob==[1,8,5,7,4]:
                    space6()
                elif ob==[1,5,8,7,6] or ob==[1,8,5,7,6]:
                    space4()   
            elif x==6: #black
                ob.append(3)
                if ob==[1]:
                    space1()
                elif ob==[1,2] or ob==[1,3] or ob==[1,4] or ob==[1,5] or ob==[1,6] or ob==[1,7] or ob==[1,8]:
