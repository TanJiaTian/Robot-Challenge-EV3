    motor_right.wait_while('running')
    motor_left.wait_while('running')
    sleep(0.3)

def grab():
    # grab action
    motor_medium.run_timed(time_sp=3500, speed_sp=-1000, stop_action="brake")
    motor_medium.wait_while('running')
    sleep(0.1)

def release():
    # release action
    motor_medium.run_timed(time_sp=3500, speed_sp=1000, stop_action="brake")
    motor_medium.wait_while('running')
    sleep(10)


def space1():
    #put block on 1
    rotate_180_right()
    grab()
    rotate_180_left()
    rotate_90_left_alt()
    striaght_to_adj()
    rotate_90_right()
    straight_3_adj()
    rotate_90_right()
    release()
    rotate_90_reverse_right()
    reverse_3_adj()
    rotate_90_reverse_right()
    reverse_to_adj()
    rotate_90_right_alt()
    sleep(30) 
    

 def space2():
    #put block on 2   
    rotate_180_right()
    grab()
    rotate_180_left()
    straight_3_middle()
    release()
    reverse_3_middle()
    sleep(30) 
    
    
def space3():
    #put block on 3
    rotate_180_right()
    grab()
    rotate_180_left()
    rotate_90_right_alt()
    striaght_to_adj()
    rotate_90_left()
    straight_3_adj()
    rotate_90_left()
    release()
    rotate_90_reverse_left()
    reverse_3_adj()
    rotate_90_reverse_left()
    reverse_to_adj()
    rotate_90_left_alt()
    sleep(30)
 
