                elif ob==[1,2,3] or ob==[1,2,4] or ob==[1,2,6] or ob==[1,2,7] or ob==[1,2,8] or ob==[1,3,2] or ob==[1,3,4] or ob==[1,3,6] or ob==[1,3,7] or ob==[1,3,8] or ob==[1,4,2] or ob==[1,4,3] or ob==[1,4,6] or ob==[1,4,7] or ob==[1,4,8] or ob==[1,6,2] or ob==[1,6,3] or ob==[1,6,4] or ob==[1,6,7] or ob==[1,6,8]or ob==[1,7,2]or ob==[1,7,3]or ob==[1,7,4]or ob==[1,7,6]or ob==[1,7,8]or ob==[1,8,2]or ob==[1,8,3]or ob==[1,8,4]or ob==[1,8,6]or ob==[1,8,7]:
                    space5()
                elif ob==[1,9,2,4] or ob==[1,2,9,4] or ob==[1,9,2,6] or ob==[1,2,9,6] or ob==[1,9,2,8] or ob==[1,2,9,8]:
