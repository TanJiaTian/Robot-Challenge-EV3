    reverse_1_adj()
    rotate_90_reverse_left()
    reverse_to_adj()
    rotate_90_left_alt()
    sleep(30) 


def game():
    while ts.value() == 1: # ts.value = 1
        for x in range(0,100):
            cl.mode = 'COL-REFLECT'
            x = cl.value()
            ob=[] #oppentents block
            if x==21-23: #no color
                ob.append(1)
                if ob==[1]:
                    space1()           
            elif x==2-3: #brown
                ob.append(2)
                if ob==[1]:
                    space1()
                elif ob==[1,2] or ob==[1,3] or ob==[1,4] or ob==[1,5] or ob==[1,6] or ob==[1,7] or ob==[1,8]:
                    space9()
                elif ob==[1,9]:
                    space3()
                elif ob==[1,9,2]:
                    space7()
                elif ob==[1,9,4] or ob==[1,9,5] or ob==[1,9,6] or ob==[1,9,7] or ob==[1,9,8]:
                    space2()
                elif ob==[1,5,2] or ob==[1,2,5]:
                    space8()
                elif ob==[1,5,3] or ob==[1,3,5]:
                    space7()
                elif ob==[1,5,4] or ob==[1,4,5]:
                    space6()
                elif ob==[1,5,6] or ob==[1,6,5]:
                    space4()
                elif ob==[1,5,7] or ob==[1,7,5]:
                    space3()
                elif ob==[1,5,8] or ob==[1,8,5]:
                    space2()
                elif ob==[1,2,3] or ob==[1,2,4] or ob==[1,2,6] or ob==[1,2,7] or ob==[1,2,8] or ob==[1,3,2] or ob==[1,3,4] or ob==[1,3,6] or ob==[1,3,7] or ob==[1,3,8] or ob==[1,4,2] or ob==[1,4,3] or ob==[1,4,6] or ob==[1,4,7] or ob==[1,4,8] or ob==[1,6,2] or ob==[1,6,3] or ob==[1,6,4] or ob==[1,6,7] or ob==[1,6,8]or ob==[1,7,2]or ob==[1,7,3]or ob==[1,7,4]or ob==[1,7,6]or ob==[1,7,8]or ob==[1,8,2]or ob==[1,8,3]or ob==[1,8,4]or ob==[1,8,6]or ob==[1,8,7]:
                    space5()
                elif ob==[1,9,2,4] or ob==[1,2,9,4] or ob==[1,9,2,6] or ob==[1,2,9,6] or ob==[1,9,2,8] or ob==[1,2,9,8]:
                    space5()
                elif ob==[1,9,2,5] or ob==[1,2,9,5]:
                    space4()
                elif ob==[1,5,2,7] or ob==[1,2,5,7]:
                    space3()
                elif ob==[1,5,2,3] or ob==[1,5,2,4] or ob==[1,5,2,6] or ob==[1,2,5,3] or ob==[1,2,5,4] or ob==[1,2,5,6]:
                    space7()
                elif ob==[1,5,3,4] or ob==[1,3,5,4] or ob==[1,5,3,2] or ob==[1,3,5,2] or ob==[1,5,3,8] or ob==[1,3,5,8]:
                    space8()
                elif ob==[1,5,3,8] or ob==[1,3,5,8]:
                    space4()
                elif ob==[1,5,4,3] or ob==[1,4,5,3]:
                    space7()
                elif ob==[1,5,4,2] or ob==[1,5,4,7] or ob==[1,5,4,8] or ob==[1,4,5,2] or ob==[1,4,5,7] or ob==[1,4,5,8]:
                    space3()
                elif ob==[1,5,6,7] or ob==[1,6,5,7]:
                    space3()
                elif ob==[1,5,6,2] or ob==[1,5,6,3] or ob==[1,5,6,8] or ob==[1,6,5,2] or ob==[1,6,5,3] or ob==[1,6,5,8]:
                    space7()
                elif ob==[1,5,7,2] or ob==[1,7,5,2] or ob==[1,5,7,4] or ob==[1,7,5,4] or ob==[1,5,7,8] or ob==[1,7,5,8]:
                    space6()
                elif ob==[1,5,7,6] or ob==[1,7,5,6]:
                    space2()
                elif ob==[1,5,8,3] or ob=[1,8,5,3]:
                    space7()
                elif ob==[1,5,8,4] or ob==[1,5,8,6] or ob==[1,5,8,7] or ob==[1,8,5,4] or ob==[1,8,5,6] or ob==[1,8,5,7]:
                    space3()
                elif ob==[1,5,2,7,6] or ob==[1,2,5,7,6]:
                    space4()
                elif ob==[1,5,2,7,4] or ob==[1,2,5,7,4]:
